package solution.javalang;

public class Employee {
	private String name;
	private double salary;
	public Employee(String name, double salary)
				{
					super();
					this.name = name;
					this.salary = salary;
				}
	String getName() 
	{
		return name;
	}
	double getSalary() 
	{
		return salary;
	}
}